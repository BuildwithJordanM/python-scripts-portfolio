import win32com.client

# Create an instance of Excel
excel = win32com.client.Dispatch('Excel.Application')
excel.Visible = True  # Set to False if you don't want to see Excel

# Open the workbook
workbook = excel.Workbooks.Open(r'C:\Users\MCJ3MTP\Downloads\White Cap Price Hold Report 11.04.24.xlsx')

# Run the macro
excel.Application.Run('PERSONAL.XLSB!Macro1')

# Save and close
workbook.Save()
workbook.Close()

# Quit Excel
excel.Quit()
